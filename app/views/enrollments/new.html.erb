<h1>New Enrollment Information:</h1>

<%= form_for @enrollment do |f| %>
  <div class="form-group">
    <%= f.label :enrollment_state_id, "Status" %>
    <%= f.collection_select(:enrollment_state_id, EnrollmentState.all, :id, :Name) %>
  </div>

  <div class="form-group">
    <%= f.label :project_id, "Project" %>
    <%= f.collection_select(:project_id, Project.all, :id, :name) %>
  </div>

  <div class="form-group">
    <%= f.label :subjId, "Subject ID" %>
    <%= f.text_field :subjId %>
  </div>

  <div class="form-group">
    <%= f.submit "Submit", class: "btn btn-primary" %>
  </div>
<% end %>
